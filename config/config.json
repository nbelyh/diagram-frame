{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "spfxvisio": {
      "components": [
        {
          "entrypoint": "./lib/WebPart/WebPart.js",
          "manifest": "./src/WebPart/WebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "VisioEmbed": {
      "path": "node_modules/@microsoft/office-js/embedded/visio-web-embedded.js",
      "globalName": "Visio"
    }
  },
  "localizedResources": {
    "WebPartStrings": "./src/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}
